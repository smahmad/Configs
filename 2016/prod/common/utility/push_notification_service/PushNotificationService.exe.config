<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configProtectedData>
		<providers>
			<add keyContainerName="CAOKey" useMachineContainer="true" description="Uses RsaCryptoServiceProvider to encrypt and decrypt" name="CAOProvider" type="System.Configuration.RsaProtectedConfigurationProvider,System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
		</providers>
  </configProtectedData>
  
  <!--Local config-->
  <connectionStrings configSource="connstrings.config" />
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
		<add key="timerInterval" 						value="60000"/>
		<add key="AWSRegion" 							value="us-west-2"/>
		<add key="BucketOutgoingNotifications" 			value="ca.prod16.push-notification"/>
		<add key="PushNotificationBatchSize" 			value="2000"/>
		<add key="ServiceName" 							value="PushNotificationService" />
		<add key="DisplayName" 							value="CA_PushNotificationService" />
		<add key="ServiceDescription" 					value="Scheduler for the PushNotificationService tasks" />
    </appSettings>  
</configuration>
