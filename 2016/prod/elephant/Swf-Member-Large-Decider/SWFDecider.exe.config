<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- General CA settings -->
    <add key="DeploymentChannel"					value="elephant" />
    <add key="AWSRegion" 							value="us-west-2" />
    <add key="SWFDomain" 							value="prod16.elephant.member" />
    <add key="SWFTaskList" 							value="Large-Tasks" />
    <add key="ServiceName" 							value="SWFLargeDecider-Member" />
    <add key="ServiceDescription" 					value="SWFLargeDecider-Member" />
    <add key="DisplayName"							value="CA Member SWF Large Decider" />
    <add key="AWSNoOfRetries" 						value="5" />
    <add key="AWSLogging" 							value="log4net"/>

		 <!-- SWF Activity-->
    <add key="GetDataForPrint" 						value="activity.print.get-data"/>
    <add key="BatchPrint" 							value="activity.print.batch"/>
    <add key="MarkAsPrinted" 						value="activity.print.mark-date"/>
    <add key="CompressPrintFiles" 					value="activity.print.compress-files"/>
    <add key="EncryptPrintFiles" 					value="activity.print.encrypt-files"/>
    <add key="CreatePrintHistory" 					value="activity.print.create-history"/>
    <add key="CopyPrintFileToPermanentLocation" 	value="activity.print.copy-to-perm"/>
    <add key="CopyPrintFileToFtp" 					value="activity.print.copy-to-ftp"/>
    <add key="CleanupPrint" 						value="activity.print.cleanup"/>
    
    <add key="GetDataForExport" 					value="activity.export.get-data"/>
    <add key="BatchExport" 							value="activity.export.batch"/>
    <add key="CreateExportHistory" 					value="activity.export.create-history"/>
    <add key="MarkAsExported" 						value="activity.export.mark-date"/>
    <add key="EncryptExportFiles" 					value="activity.export.encrypt-files"/>
    <add key="CopyExportFileToPermanentLocation" 	value="activity.export.copy-to-perm"/>
    <add key="CopyExportFileToFtp" 					value="activity.export.copy-to-ftp"/>
    <add key="CleanupExport" 						value="activity.export.cleanup"/>

	<add key="SWFLargeExportWorkflow" 				value="workflow.export.large"/>
	<add key="SWFSmallExportWorkflow" 				value="workflow.export.small"/>
	<add key="SWFSmallPrintWorkflow" 				value="workflow.print.small"/>
	<add key="SWFLargePrintWorkflow" 				value="workflow.print.large"/>

    <!--SWF Activity Large Print work flow configs - Version,ScheduleToCloseTimeout,ScheduleToStartTimeout,StartToCloseTimeout -->
    <add key="LargePrintGetDataForPrintConfig" 					value="1.0,21600,3000,3000"/>
    <add key="LargePrintBatchPrintConfig" 						value="1.0,21600,21600,21600"/>
    <add key="LargePrintCompressPrintFilesConfig" 				value="1.0,21600,21600,21600"/>
    <add key="LargePrintEncryptPrintFilesConfig" 				value="1.0,21600,21600,21600"/>
    <add key="LargePrintCreatePrintHistoryConfig" 				value="1.0,21600,3000,3000"/>
    <add key="LargePrintCopyPrintFileToPermanentLocationConfig" value="1.0,21600,3000,3000"/>
    <add key="LargePrintMarkAsPrintedConfig" 					value="1.0,21600,3000,3000"/>
    <add key="LargePrintCopyPrintFileToFtpConfig" 				value="1.0,21600,3000,3000"/>
    <add key="LargePrintCleanupPrintConfig"						value="1.0,21600,3000,3000"/>

    <!--SWF Activity Large Export work flow configs - Version,ScheduleToCloseTimeout,ScheduleToStartTimeout,StartToCloseTimeout -->
    <add key="LargeExportGetDataForExportConfig" 					value="1.0,21600,3000,3000"/>
    <add key="LargeExportBatchExportConfig" 						value="1.0,21600,21600,21600"/>
    <add key="LargeExportEncryptExportFilesConfig"  				value="1.0,21600,3000,3000"/>
    <add key="LargeExportCreateExportHistoryConfig" 				value="1.0,21600,3000,3000"/>
    <add key="LargeExportCopyExportFileToPermanentLocationConfig" 	value="1.0,21600,3000,3000"/>
    <add key="LargeExportMarkAsExportedConfig" 						value="1.0,21600,3000,3000"/>
    <add key="LargeExportCopyExportFileToFtpConfig" 				value="1.0,21600,3000,3000"/>
    <add key="LargeExportCleanupExportConfig" 						value="1.0,21600,3000,3000"/>

    <!--SWF Activity Small Export work flow configs - Version,ScheduleToCloseTimeout,ScheduleToStartTimeout,StartToCloseTimeout -->
    <add key="SmallExportGetDataForExportConfig" 			value="1.0,1800,1200,1200"/>
    <add key="SmallExportBatchExportConfig" 				value="1.0,4200,3600,3600"/>
    <add key="SmallExportMarkAsExportedConfig" 				value="1.0,1800,1200,1200"/>
    <add key="SmallExportCleanupExportConfig" 				value="1.0,1800,1200,1200"/>

    <!--SWF Activity Small Print work flow configs - Version,ScheduleToCloseTimeout,ScheduleToStartTimeout,StartToCloseTimeout -->
    <add key="SmallPrintGetDataForPrintConfig" 				value="1.0,1800,1200,1200"/>
    <add key="SmallPrintBatchPrintConfig" 					value="1.0,4200,3600,3600"/>
    <add key="SmallPrintMarkAsPrintedConfig" 				value="1.0,1800,1200,1200"/>
    <add key="SmallPrintCleanupPrintConfig" 				value="1.0,1800,1200,1200"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
